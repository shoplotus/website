        "text_FLeHwf": {
          "type": "text",
          "settings": {
            "quote_content": "<h3>Like having a very specific superpower I wouldn’t mind using in my daily life.<\/h3>",
            "quote_author": "Chris Velazco",
            "quote_caption": "Personal Technology Reporter",
            "quote_avatar": "shopify:\/\/shop_images\/washington-post-logo.png"
